{
  "name": "My First Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "background",
    "https://ajax.googleapis.com/",
    "http://datawallet.jd.com/",
    "http://vip.m.jd.com/"
  ]
}
